{
  "env": {
    "development": {
      "presets": [
        "es2015",
        "stage-0"
      ],
      "plugins": [
        "add-module-exports",
        "transform-async-to-generator",
        "transform-runtime",
        ["component", [
          {
            "libraryName": "element-ui",
            "styleLibraryName": "theme-chalk"
          }
        ]]
      ],
      "comments": false
    },
    "production": {
      "presets": [
        "es2015",
        "stage-0"
      ],
      "plugins": [
        "add-module-exports",
        "transform-async-to-generator",
        "transform-runtime",
        ["component", [
          {
            "libraryName": "element-ui",
            "styleLibraryName": "theme-chalk"
          }
        ]]
      ],
      "comments": false
    }
  }
}
